{
  "name": "automation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:parallel": "cypress-parallel -s cy:run -t 1 -d cypress/e2e/other/qa/ -r mochawesome",
    "cy:run": "cypress run",
    "cy:run_spec": "cypress run --spec cypress/e2e/other/qaclickacademy.js",
    "cy:run_allbrowsers": "npm run cy:run_spec -- --browser chrome & npm run cy:run_spec -- --browser electron & npm run cy:run_spec -- --browser edge & npm run cy:run_spec -- --browser firefox",
    "triggerAllTest-headed": "npx cypress run --headed --browser edge",
    "cy:run-webdriveruni": "npx cypress run --spec cypress/e2e/webdriver-uni/* --headless",
    "cy:run-autoTestStore": "npx cypress run --spec cypress/e2e/automation-test-store/* --headless",
    "cy:run-autoTestStore-dashboard": "npx cypress run --spec cypress/e2e/automation-test-store/* --record --key 1cd01925-701c-42ec-8fad-ecab986ddd34",
    "junit-merge": "npx junit-merge -d cypress/results/junit -o cypress/results/junit/results.xml",
    "delete-junit-report": "rm -rf cypress/results/junit/results.xml",
    "delete-all-reports": "rm -rf cypress/results/* || true",
    "mochawesome-merge": "npx mochawesome-merge cypress/results/mochawesome/*.json > mochawesome.json && npx marge mochawesome.json -i true",
    "delete-mochawesome-report": "rm -rf mochawesome-report/* || true",
    "cypress-regression-pack": "npm run delete-all-reports && npm run delete-mochawesome-report && npx cypress run && npm run mochawesome-merge",
    "cypress-multi-browser": "npm run triggerAllTest-autoTestStore -- --browser electron --headed & npm run triggerAllTest-autoTestStore -- --browser edge --headed ",
    "triggerAllTests-cucumberTests": "npx cypress run --spec cypress/e2e/features/*.feature -b edge",
    "cy:run_qaclickacademy": "npx cypress run  --spec cypress/e2e/other/qaclickacademy.js"
  },
  "author": "bgd",
  "license": "ISC",
  "dependencies": {
    "@cypress/webpack-preprocessor": "latest",
    "autoprefixer": "^10.4.8",
    "cypress-parallel": "^0.9.1",
    "esbuild": "^0.15.5",
    "npx": "^10.2.2"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/features/**/*.{js,ts}",
    "filterSpecs": true,
    "messages.enabled": true,
    "messagesOutput": "cucumber-messages.ndjson",
    "jsonFormatter": "cucumber-json-formatter"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^12.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "cypress": "^10.6.0",
    "cypress-multi-reporters": "^1.6.1",
    "cypress-xpath": "^2.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "mocha": "^10.0.0",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0"
  }
}
