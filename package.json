{
  "name": "automation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "triggerAllTest-headed": "npx cypress run --headed --browser edge",
    "triggerAllTest-webdriveruni": "npx cypress run --spec cypress/integration/webdriver-uni/* --headed --browser edge",
    "triggerAllTest-autoTestStore": "npx cypress run --spec cypress/integration/automation-test-store/* --headed --browser edge",
    "junit-merge": "npx junit-merge -d cypress/results/junit -o cypress/results/junit/results.xml",
    "delete-junit-report": "rm -rf cypress/results/junit/results.xml",
    "delete-all-reports": "rm -rf cypress/results/* || true",
    "mochawesome-merge": "npx mochawesome-merge cypress/results/mochawesome/*.json > mochawesome.json && npx marge mochawesome.json -i true",
    "delete-mochawesome-report": "rm -rf mochawesome-report/* || true",
    "cypress-regression-pack": "npm run delete-all-results && npm run delete-mochawesome-report && npx cypress run && npm run mochawesome-merge"
  },
  "author": "bgd",
  "license": "ISC",
  "dependencies": {
    "cypress-file-upload": "^5.0.8",
    "cypress-real-events": "^1.7.0",
    "npx": "^10.2.2"
  },
  "devDependencies": {
    "cypress": "^9.5.4",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-xpath": "^1.6.2",
    "mocha": "^9.2.2",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0"
  }
}
